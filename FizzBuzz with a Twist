let num = parseInt(inp); // Don't change this line

console.log("Welcome to FizzBuzz!");

function fizzbuzz(number) {
    let result = ""
    if (number % 3 == 0) {
        result += "Fizz"
    }
    if (number % 7 == 0) {
        result += "Buzz"
    }
    if (result == "") {
        result = String(number)
        if (result.includes("3")) {
            result = "Almost Fizz"
        }
    }
    return result
}

for (let i = 1; i <= num; i++) {
    console.log(fizzbuzz(i))
}
